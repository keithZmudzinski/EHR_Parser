<table class="table table-striped table-hover table-condensed table-responsive">
  <caption>Disorders</caption>
  <thead>
    <th>Label</th>
    <th>CUI</th>
    <th>Ontology</th>
    <th>Context</th>
  </thead>
  <tbody>
    <tr ng-repeat="disorder in _extractDisorder.instances">
      <td class="no-wrap">{{disorder.term}}</td>
      <td class="no-wrap">{{disorder.cui}}</td>
      <td class="no-wrap">{{disorder.onto}}</td>
      <td class="word-wrap">{{disorder.context.replace("\t","*")}}</td>
    </tr>
  </tbody>
</table>
