<table class="table table-striped table-hover table-condensed table-responsive">
  <caption>Congestive heart failure terms</caption>
  <thead>
    <th>Term</th>
    <th>Type</th>
    <th>Context</th>
  </thead>
  <tbody>
    <tr ng-repeat="chf in _extractChf.instances">
      <td class="no-wrap">{{chf.trigger}}</td>
      <td class="no-wrap">{{chf.type}}</td>
      <td class="word-wrap">{{chf.context.replace("\t","*")}}</td>
    </tr>
  </tbody>
</table>
