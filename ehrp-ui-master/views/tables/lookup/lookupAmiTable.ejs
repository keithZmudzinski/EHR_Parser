<table class="table table-striped table-hover table-condensed table-responsive">
  <caption>Acute Myocardial Infarction terms</caption>
  <thead>
    <th>Term</th>
    <th>Type</th>
  </thead>
  <tbody>
    <tr ng-repeat="ami in _lookupAmi.instances">
      <td class="no-wrap">{{ami.trigger}}</td>
      <td class="no-wrap">{{ami.type}}</td>
    </tr>
  </tbody>
</table>
